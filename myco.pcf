# For i4004 ice40LP1k-CM36 board

set_io sw1 E1 -pullup yes
set_io sw2 D1 -pullup yes
set_io dout[0] B5 
set_io dout[1] B6 
set_io dout[2] C6 
set_io dout[3] E6 
set_io din[0] B1 -pullup yes
set_io din[1] A1 -pullup yes
set_io din[2] A2 -pullup yes
set_io din[3] A3 -pullup yes
set_io pwm_out C5
set_io clock F2
set_io n_reset B4 -pullup yes



